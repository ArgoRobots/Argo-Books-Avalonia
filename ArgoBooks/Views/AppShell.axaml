<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:ArgoBooks.ViewModels"
             xmlns:controls="using:ArgoBooks.Controls"
             mc:Ignorable="d" d:DesignWidth="1280" d:DesignHeight="800"
             x:Class="ArgoBooks.Views.AppShell"
             x:DataType="vm:AppShellViewModel"
             Background="{DynamicResource BackgroundBrush}">

    <Design.DataContext>
        <vm:AppShellViewModel />
    </Design.DataContext>

    <Grid ColumnDefinitions="Auto,*">
        <!-- Sidebar -->
        <Border Grid.Column="0"
                Background="{DynamicResource SidebarBackgroundBrush}"
                Width="{Binding SidebarWidth}"
                BorderBrush="{DynamicResource BorderBrush}"
                BorderThickness="0,0,1,0">
            <DockPanel>
                <!-- Sidebar Header -->
                <Border DockPanel.Dock="Top"
                        Padding="16,12"
                        BorderBrush="{DynamicResource BorderBrush}"
                        BorderThickness="0,0,0,1">
                    <Grid ColumnDefinitions="*,Auto">
                        <!-- Company Logo/Name -->
                        <StackPanel Grid.Column="0"
                                    Orientation="Horizontal"
                                    Spacing="12"
                                    IsVisible="{Binding !IsSidebarCollapsed}">
                            <Border Width="32" Height="32"
                                    CornerRadius="{StaticResource RadiusSm}"
                                    Background="{DynamicResource PrimaryBrush}">
                                <TextBlock Text="A"
                                           FontSize="{StaticResource FontSizeLg}"
                                           FontWeight="Bold"
                                           Foreground="{DynamicResource TextInverseBrush}"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center" />
                            </Border>
                            <TextBlock Text="{Binding CompanyName, FallbackValue='Argo Books'}"
                                       FontSize="{StaticResource FontSizeMd}"
                                       FontWeight="SemiBold"
                                       Foreground="{DynamicResource SidebarTextBrush}"
                                       VerticalAlignment="Center"
                                       MaxWidth="140"
                                       TextTrimming="CharacterEllipsis" />
                        </StackPanel>

                        <!-- Collapse Button -->
                        <Button Grid.Column="1"
                                Classes="icon"
                                Width="28" Height="28"
                                Command="{Binding ToggleSidebarCommand}"
                                ToolTip.Tip="{Binding SidebarToggleTooltip}">
                            <PathIcon Width="16" Height="16"
                                      Foreground="{DynamicResource SidebarTextMutedBrush}">
                                <PathIcon.Styles>
                                    <Style Selector="PathIcon.expanded">
                                        <Setter Property="Data" Value="M15.41 7.41L14 6l-6 6 6 6 1.41-1.41L10.83 12z" />
                                    </Style>
                                    <Style Selector="PathIcon.collapsed">
                                        <Setter Property="Data" Value="M10 6L8.59 7.41 13.17 12l-4.58 4.59L10 18l6-6z" />
                                    </Style>
                                </PathIcon.Styles>
                                <PathIcon.Classes>
                                    <MultiBinding Converter="{x:Static BoolConverters.Or}">
                                        <Binding Path="IsSidebarCollapsed" />
                                    </MultiBinding>
                                </PathIcon.Classes>
                            </PathIcon>
                        </Button>
                    </Grid>
                </Border>

                <!-- Sidebar Footer -->
                <Border DockPanel.Dock="Bottom"
                        Padding="8"
                        BorderBrush="{DynamicResource BorderBrush}"
                        BorderThickness="0,1,0,0">
                    <StackPanel Spacing="4">
                        <!-- Settings Button -->
                        <Button Classes="ghost"
                                Command="{Binding OpenSettingsCommand}"
                                HorizontalAlignment="Stretch"
                                Padding="8">
                            <StackPanel Orientation="Horizontal" Spacing="12">
                                <PathIcon Data="M19.14 12.94c.04-.31.06-.63.06-.94s-.02-.63-.06-.94l2.03-1.58c.18-.14.23-.41.12-.61l-1.92-3.32c-.12-.22-.37-.29-.59-.22l-2.39.96c-.5-.38-1.03-.7-1.62-.94l-.36-2.54c-.04-.24-.24-.41-.48-.41h-3.84c-.24 0-.43.17-.47.41l-.36 2.54c-.59.24-1.13.57-1.62.94l-2.39-.96c-.22-.08-.47 0-.59.22L2.74 8.87c-.12.21-.08.47.12.61l2.03 1.58c-.04.31-.06.63-.06.94s.02.63.06.94l-2.03 1.58c-.18.14-.23.41-.12.61l1.92 3.32c.12.22.37.29.59.22l2.39-.96c.5.38 1.03.7 1.62.94l.36 2.54c.05.24.24.41.48.41h3.84c.24 0 .44-.17.47-.41l.36-2.54c.59-.24 1.13-.56 1.62-.94l2.39.96c.22.08.47 0 .59-.22l1.92-3.32c.12-.22.07-.47-.12-.61l-2.01-1.58zM12 15.6c-1.98 0-3.6-1.62-3.6-3.6s1.62-3.6 3.6-3.6 3.6 1.62 3.6 3.6-1.62 3.6-3.6 3.6z"
                                          Width="18" Height="18"
                                          Foreground="{DynamicResource SidebarTextMutedBrush}" />
                                <TextBlock Text="Settings"
                                           FontSize="{StaticResource FontSizeSm}"
                                           Foreground="{DynamicResource SidebarTextMutedBrush}"
                                           VerticalAlignment="Center"
                                           IsVisible="{Binding !IsSidebarCollapsed}" />
                            </StackPanel>
                        </Button>

                        <!-- Help Button -->
                        <Button Classes="ghost"
                                Command="{Binding OpenHelpCommand}"
                                HorizontalAlignment="Stretch"
                                Padding="8">
                            <StackPanel Orientation="Horizontal" Spacing="12">
                                <PathIcon Data="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 17h-2v-2h2v2zm2.07-7.75l-.9.92C13.45 12.9 13 13.5 13 15h-2v-.5c0-1.1.45-2.1 1.17-2.83l1.24-1.26c.37-.36.59-.86.59-1.41 0-1.1-.9-2-2-2s-2 .9-2 2H8c0-2.21 1.79-4 4-4s4 1.79 4 4c0 .88-.36 1.68-.93 2.25z"
                                          Width="18" Height="18"
                                          Foreground="{DynamicResource SidebarTextMutedBrush}" />
                                <TextBlock Text="Help"
                                           FontSize="{StaticResource FontSizeSm}"
                                           Foreground="{DynamicResource SidebarTextMutedBrush}"
                                           VerticalAlignment="Center"
                                           IsVisible="{Binding !IsSidebarCollapsed}" />
                            </StackPanel>
                        </Button>
                    </StackPanel>
                </Border>

                <!-- Navigation Items -->
                <ScrollViewer VerticalScrollBarVisibility="Auto"
                              HorizontalScrollBarVisibility="Disabled"
                              Padding="8,12">
                    <StackPanel Spacing="4">
                        <!-- Main Section -->
                        <TextBlock Text="MAIN"
                                   FontSize="{StaticResource FontSizeXs}"
                                   FontWeight="SemiBold"
                                   Foreground="{DynamicResource SidebarTextMutedBrush}"
                                   Margin="8,0,0,8"
                                   IsVisible="{Binding !IsSidebarCollapsed}" />

                        <!-- Dashboard -->
                        <Button Classes="sidebar-item"
                                Classes.active="{Binding IsPageActive[Dashboard]}"
                                Command="{Binding NavigateCommand}"
                                CommandParameter="Dashboard">
                            <StackPanel Orientation="Horizontal" Spacing="12">
                                <PathIcon Data="M3 13h8V3H3v10zm0 8h8v-6H3v6zm10 0h8V11h-8v10zm0-18v6h8V3h-8z"
                                          Width="18" Height="18" />
                                <TextBlock Text="Dashboard"
                                           IsVisible="{Binding !IsSidebarCollapsed}" />
                            </StackPanel>
                        </Button>

                        <!-- Analytics -->
                        <Button Classes="sidebar-item"
                                Classes.active="{Binding IsPageActive[Analytics]}"
                                Command="{Binding NavigateCommand}"
                                CommandParameter="Analytics">
                            <StackPanel Orientation="Horizontal" Spacing="12">
                                <PathIcon Data="M19 3H5c-1.1 0-2 .9-2 2v14c0 1.1.9 2 2 2h14c1.1 0 2-.9 2-2V5c0-1.1-.9-2-2-2zM9 17H7v-7h2v7zm4 0h-2V7h2v10zm4 0h-2v-4h2v4z"
                                          Width="18" Height="18" />
                                <TextBlock Text="Analytics"
                                           IsVisible="{Binding !IsSidebarCollapsed}" />
                            </StackPanel>
                        </Button>

                        <!-- Transactions Section -->
                        <TextBlock Text="TRANSACTIONS"
                                   FontSize="{StaticResource FontSizeXs}"
                                   FontWeight="SemiBold"
                                   Foreground="{DynamicResource SidebarTextMutedBrush}"
                                   Margin="8,16,0,8"
                                   IsVisible="{Binding !IsSidebarCollapsed}" />

                        <!-- Revenue -->
                        <Button Classes="sidebar-item"
                                Classes.active="{Binding IsPageActive[Revenue]}"
                                Command="{Binding NavigateCommand}"
                                CommandParameter="Revenue">
                            <StackPanel Orientation="Horizontal" Spacing="12">
                                <PathIcon Data="M11.8 10.9c-2.27-.59-3-1.2-3-2.15 0-1.09 1.01-1.85 2.7-1.85 1.78 0 2.44.85 2.5 2.1h2.21c-.07-1.72-1.12-3.3-3.21-3.81V3h-3v2.16c-1.94.42-3.5 1.68-3.5 3.61 0 2.31 1.91 3.46 4.7 4.13 2.5.6 3 1.48 3 2.41 0 .69-.49 1.79-2.7 1.79-2.06 0-2.87-.92-2.98-2.1h-2.2c.12 2.19 1.76 3.42 3.68 3.83V21h3v-2.15c1.95-.37 3.5-1.5 3.5-3.55 0-2.84-2.43-3.81-4.7-4.4z"
                                          Width="18" Height="18" />
                                <TextBlock Text="Revenue"
                                           IsVisible="{Binding !IsSidebarCollapsed}" />
                            </StackPanel>
                        </Button>

                        <!-- Expenses -->
                        <Button Classes="sidebar-item"
                                Classes.active="{Binding IsPageActive[Expenses]}"
                                Command="{Binding NavigateCommand}"
                                CommandParameter="Expenses">
                            <StackPanel Orientation="Horizontal" Spacing="12">
                                <PathIcon Data="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1.41 16.09V20h-2.67v-1.93c-1.71-.36-3.16-1.46-3.27-3.4h1.96c.1 1.05.82 1.87 2.65 1.87 1.96 0 2.4-.98 2.4-1.59 0-.83-.44-1.61-2.67-2.14-2.48-.6-4.18-1.62-4.18-3.67 0-1.72 1.39-2.84 3.11-3.21V4h2.67v1.95c1.86.45 2.79 1.86 2.85 3.39H14.3c-.05-1.11-.64-1.87-2.22-1.87-1.5 0-2.4.68-2.4 1.64 0 .84.65 1.39 2.67 1.91s4.18 1.39 4.18 3.91c-.01 1.83-1.38 2.83-3.12 3.16z"
                                          Width="18" Height="18" />
                                <TextBlock Text="Expenses"
                                           IsVisible="{Binding !IsSidebarCollapsed}" />
                            </StackPanel>
                        </Button>

                        <!-- Invoices -->
                        <Button Classes="sidebar-item"
                                Classes.active="{Binding IsPageActive[Invoices]}"
                                Command="{Binding NavigateCommand}"
                                CommandParameter="Invoices">
                            <StackPanel Orientation="Horizontal" Spacing="12">
                                <PathIcon Data="M14 2H6c-1.1 0-1.99.9-1.99 2L4 20c0 1.1.89 2 1.99 2H18c1.1 0 2-.9 2-2V8l-6-6zm2 16H8v-2h8v2zm0-4H8v-2h8v2zm-3-5V3.5L18.5 9H13z"
                                          Width="18" Height="18" />
                                <TextBlock Text="Invoices"
                                           IsVisible="{Binding !IsSidebarCollapsed}" />
                            </StackPanel>
                        </Button>

                        <!-- Inventory Section -->
                        <TextBlock Text="INVENTORY"
                                   FontSize="{StaticResource FontSizeXs}"
                                   FontWeight="SemiBold"
                                   Foreground="{DynamicResource SidebarTextMutedBrush}"
                                   Margin="8,16,0,8"
                                   IsVisible="{Binding !IsSidebarCollapsed}" />

                        <!-- Products -->
                        <Button Classes="sidebar-item"
                                Classes.active="{Binding IsPageActive[Products]}"
                                Command="{Binding NavigateCommand}"
                                CommandParameter="Products">
                            <StackPanel Orientation="Horizontal" Spacing="12">
                                <PathIcon Data="M20 2H4c-1 0-2 .9-2 2v3.01c0 .72.43 1.34 1 1.69V20c0 1.1 1.1 2 2 2h14c.9 0 2-.9 2-2V8.7c.57-.35 1-.97 1-1.69V4c0-1.1-1-2-2-2zm-5 12H9v-2h6v2zm5-7H4V4h16v3z"
                                          Width="18" Height="18" />
                                <TextBlock Text="Products"
                                           IsVisible="{Binding !IsSidebarCollapsed}" />
                            </StackPanel>
                        </Button>

                        <!-- Stock Levels -->
                        <Button Classes="sidebar-item"
                                Classes.active="{Binding IsPageActive[StockLevels]}"
                                Command="{Binding NavigateCommand}"
                                CommandParameter="StockLevels">
                            <StackPanel Orientation="Horizontal" Spacing="12">
                                <PathIcon Data="M19 3H5c-1.1 0-2 .9-2 2v14c0 1.1.9 2 2 2h14c1.1 0 2-.9 2-2V5c0-1.1-.9-2-2-2zm0 16H5V5h14v14zM7 10h2v7H7zm4-3h2v10h-2zm4 6h2v4h-2z"
                                          Width="18" Height="18" />
                                <TextBlock Text="Stock Levels"
                                           IsVisible="{Binding !IsSidebarCollapsed}" />
                            </StackPanel>
                        </Button>

                        <!-- Contacts Section -->
                        <TextBlock Text="CONTACTS"
                                   FontSize="{StaticResource FontSizeXs}"
                                   FontWeight="SemiBold"
                                   Foreground="{DynamicResource SidebarTextMutedBrush}"
                                   Margin="8,16,0,8"
                                   IsVisible="{Binding !IsSidebarCollapsed}" />

                        <!-- Customers -->
                        <Button Classes="sidebar-item"
                                Classes.active="{Binding IsPageActive[Customers]}"
                                Command="{Binding NavigateCommand}"
                                CommandParameter="Customers">
                            <StackPanel Orientation="Horizontal" Spacing="12">
                                <PathIcon Data="M16 11c1.66 0 2.99-1.34 2.99-3S17.66 5 16 5c-1.66 0-3 1.34-3 3s1.34 3 3 3zm-8 0c1.66 0 2.99-1.34 2.99-3S9.66 5 8 5C6.34 5 5 6.34 5 8s1.34 3 3 3zm0 2c-2.33 0-7 1.17-7 3.5V19h14v-2.5c0-2.33-4.67-3.5-7-3.5zm8 0c-.29 0-.62.02-.97.05 1.16.84 1.97 1.97 1.97 3.45V19h6v-2.5c0-2.33-4.67-3.5-7-3.5z"
                                          Width="18" Height="18" />
                                <TextBlock Text="Customers"
                                           IsVisible="{Binding !IsSidebarCollapsed}" />
                            </StackPanel>
                        </Button>

                        <!-- Suppliers -->
                        <Button Classes="sidebar-item"
                                Classes.active="{Binding IsPageActive[Suppliers]}"
                                Command="{Binding NavigateCommand}"
                                CommandParameter="Suppliers">
                            <StackPanel Orientation="Horizontal" Spacing="12">
                                <PathIcon Data="M20 8h-3V4H3c-1.1 0-2 .9-2 2v11h2c0 1.66 1.34 3 3 3s3-1.34 3-3h6c0 1.66 1.34 3 3 3s3-1.34 3-3h2v-5l-3-4zM6 18.5c-.83 0-1.5-.67-1.5-1.5s.67-1.5 1.5-1.5 1.5.67 1.5 1.5-.67 1.5-1.5 1.5zm13.5-9l1.96 2.5H17V9.5h2.5zm-1.5 9c-.83 0-1.5-.67-1.5-1.5s.67-1.5 1.5-1.5 1.5.67 1.5 1.5-.67 1.5-1.5 1.5z"
                                          Width="18" Height="18" />
                                <TextBlock Text="Suppliers"
                                           IsVisible="{Binding !IsSidebarCollapsed}" />
                            </StackPanel>
                        </Button>

                        <!-- Employees -->
                        <Button Classes="sidebar-item"
                                Classes.active="{Binding IsPageActive[Employees]}"
                                Command="{Binding NavigateCommand}"
                                CommandParameter="Employees">
                            <StackPanel Orientation="Horizontal" Spacing="12">
                                <PathIcon Data="M12 12c2.21 0 4-1.79 4-4s-1.79-4-4-4-4 1.79-4 4 1.79 4 4 4zm0 2c-2.67 0-8 1.34-8 4v2h16v-2c0-2.66-5.33-4-8-4z"
                                          Width="18" Height="18" />
                                <TextBlock Text="Employees"
                                           IsVisible="{Binding !IsSidebarCollapsed}" />
                            </StackPanel>
                        </Button>
                    </StackPanel>
                </ScrollViewer>
            </DockPanel>
        </Border>

        <!-- Main Content Area -->
        <Grid Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <!-- Header -->
            <Border Grid.Row="0"
                    Background="{DynamicResource HeaderBackgroundBrush}"
                    BorderBrush="{DynamicResource HeaderBorderBrush}"
                    BorderThickness="0,0,0,1"
                    Padding="24,12">
                <Grid ColumnDefinitions="*,Auto,Auto,Auto,Auto">
                    <!-- Global Search -->
                    <Border Grid.Column="0"
                            Background="{DynamicResource InputBackgroundBrush}"
                            BorderBrush="{DynamicResource InputBorderBrush}"
                            BorderThickness="1"
                            CornerRadius="{StaticResource RadiusMd}"
                            MaxWidth="400"
                            HorizontalAlignment="Left">
                        <Grid ColumnDefinitions="Auto,*">
                            <PathIcon Grid.Column="0"
                                      Data="M15.5 14h-.79l-.28-.27C15.41 12.59 16 11.11 16 9.5 16 5.91 13.09 3 9.5 3S3 5.91 3 9.5 5.91 16 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z"
                                      Width="16" Height="16"
                                      Foreground="{DynamicResource TextTertiaryBrush}"
                                      Margin="12,0,8,0" />
                            <TextBox Grid.Column="1"
                                     Watermark="Search..."
                                     BorderThickness="0"
                                     Background="Transparent"
                                     Padding="0,8,12,8"
                                     Text="{Binding SearchQuery}" />
                        </Grid>
                    </Border>

                    <!-- Quick Actions -->
                    <Button Grid.Column="1"
                            Classes="icon"
                            Width="36" Height="36"
                            Margin="8,0"
                            Command="{Binding OpenQuickActionsCommand}"
                            ToolTip.Tip="Quick Actions (Ctrl+K)">
                        <PathIcon Data="M19 13h-6v6h-2v-6H5v-2h6V5h2v6h6v2z"
                                  Width="20" Height="20"
                                  Foreground="{DynamicResource TextSecondaryBrush}" />
                    </Button>

                    <!-- Notifications -->
                    <Button Grid.Column="2"
                            Classes="icon"
                            Width="36" Height="36"
                            Margin="8,0"
                            Command="{Binding OpenNotificationsCommand}"
                            ToolTip.Tip="Notifications">
                        <Panel>
                            <PathIcon Data="M12 22c1.1 0 2-.9 2-2h-4c0 1.1.89 2 2 2zm6-6v-5c0-3.07-1.64-5.64-4.5-6.32V4c0-.83-.67-1.5-1.5-1.5s-1.5.67-1.5 1.5v.68C7.63 5.36 6 7.92 6 11v5l-2 2v1h16v-1l-2-2z"
                                      Width="20" Height="20"
                                      Foreground="{DynamicResource TextSecondaryBrush}" />
                            <!-- Notification Badge -->
                            <Border Background="{DynamicResource DangerBrush}"
                                    Width="8" Height="8"
                                    CornerRadius="4"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Top"
                                    Margin="0,4,4,0"
                                    IsVisible="{Binding HasUnreadNotifications}" />
                        </Panel>
                    </Button>

                    <!-- User Menu -->
                    <Button Grid.Column="3"
                            Classes="icon"
                            Width="36" Height="36"
                            Margin="8,0,0,0"
                            Command="{Binding OpenUserMenuCommand}"
                            ToolTip.Tip="User Menu">
                        <PathIcon Data="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm0 3c1.66 0 3 1.34 3 3s-1.34 3-3 3-3-1.34-3-3 1.34-3 3-3zm0 14.2c-2.5 0-4.71-1.28-6-3.22.03-1.99 4-3.08 6-3.08 1.99 0 5.97 1.09 6 3.08-1.29 1.94-3.5 3.22-6 3.22z"
                                  Width="20" Height="20"
                                  Foreground="{DynamicResource TextSecondaryBrush}" />
                    </Button>
                </Grid>
            </Border>

            <!-- Page Content -->
            <Border Grid.Row="1"
                    Background="{DynamicResource BackgroundSecondaryBrush}">
                <ContentControl Content="{Binding CurrentPage}"
                                Margin="24" />
            </Border>
        </Grid>
    </Grid>

    <UserControl.Styles>
        <!-- Sidebar Item Style -->
        <Style Selector="Button.sidebar-item">
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="Foreground" Value="{DynamicResource SidebarTextMutedBrush}" />
            <Setter Property="Padding" Value="12,10" />
            <Setter Property="HorizontalAlignment" Value="Stretch" />
            <Setter Property="HorizontalContentAlignment" Value="Left" />
            <Setter Property="CornerRadius" Value="{StaticResource RadiusSm}" />
            <Setter Property="Cursor" Value="Hand" />
        </Style>
        <Style Selector="Button.sidebar-item:pointerover /template/ ContentPresenter#PART_ContentPresenter">
            <Setter Property="Background" Value="{DynamicResource SidebarHoverBrush}" />
        </Style>
        <Style Selector="Button.sidebar-item:pressed /template/ ContentPresenter#PART_ContentPresenter">
            <Setter Property="Background" Value="{DynamicResource SidebarHoverBrush}" />
        </Style>
        <Style Selector="Button.sidebar-item.active">
            <Setter Property="Background" Value="{DynamicResource SidebarActiveBrush}" />
            <Setter Property="Foreground" Value="{DynamicResource TextInverseBrush}" />
        </Style>
        <Style Selector="Button.sidebar-item.active PathIcon">
            <Setter Property="Foreground" Value="{DynamicResource TextInverseBrush}" />
        </Style>
        <Style Selector="Button.sidebar-item PathIcon">
            <Setter Property="Foreground" Value="{DynamicResource SidebarTextMutedBrush}" />
        </Style>
        <Style Selector="Button.sidebar-item TextBlock">
            <Setter Property="FontSize" Value="{StaticResource FontSizeSm}" />
            <Setter Property="VerticalAlignment" Value="Center" />
        </Style>
    </UserControl.Styles>
</UserControl>
